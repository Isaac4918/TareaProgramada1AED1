<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="900.0" prefWidth="1500.0" style="-fx-background-image: url('file:\fondo.png');">
            <effect>
                <InnerShadow />
            </effect>
            <children>
                <Button layoutX="1093.0" layoutY="386.0" mnemonicParsing="false" onMousePressed="#CREATEPLAY" prefHeight="115.0" prefWidth="250.0" text="CREAR PARTIDA" textFill="RED">
                    <font>
                        <Font name="Verdana" size="21.0" />
                    </font>
                    <effect>
                        <InnerShadow blurType="GAUSSIAN" choke="0.64" height="23.72" radius="11.36" width="23.72" />
                    </effect>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
                <Button layoutX="1093.0" layoutY="546.0" mnemonicParsing="false" onMousePressed="#JOINPLAY" prefHeight="104.0" prefWidth="250.0" text="UNIRSE A PARTIDA" textFill="#080e41">
                    <font>
                        <Font name="Verdana" size="21.0" />
                    </font>
                    <effect>
                        <InnerShadow choke="0.64" height="23.72" radius="11.36" width="23.72" />
                    </effect>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
            </children></AnchorPane>
    </children>
</GridPane>
